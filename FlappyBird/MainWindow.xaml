<Window x:Class="FlappyBird.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlappyBird"
        mc:Ignorable="d"
        KeyDown="Window_KeyDown"
        Title="Flappy Bird" Height="600" Width="800" WindowStartupLocation="CenterScreen">

    <!--This is the start of my world.-->
    <Canvas Background="LightSkyBlue">
        <Image x:Name="bird" Source="bird.png" Stretch="Fill" Height="69" Canvas.Left="63" Canvas.Top="135" Width="73"/>
        <Image x:Name="pipe" Height="572" Source="pipe2.jpg" Stretch="Fill" Width="84" Canvas.Left="322"/>
        <Image x:Name="pipe2" Height="559" Canvas.Left="708" Source="pipe2.jpg" Stretch="Fill" Width="84"/>
        <Image x:Name="pipe3" Height="559" Source="pipe2.jpg" Stretch="Fill" Width="84" Canvas.Left="322"/>
        <Image x:Name="pipe4" Height="572" Canvas.Left="708" Source="pipe2.jpg" Stretch="Fill" Width="84"/>
        <TextBox Background="LawnGreen" x:Name="timer" Height="33" Text="TextBox" BorderThickness="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="20" FontWeight="Bold" Width="56" Canvas.Left="366" Canvas.Top="443"/>
    </Canvas>
</Window>
